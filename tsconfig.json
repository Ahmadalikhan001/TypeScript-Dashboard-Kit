{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "*": ["node_modules/*"]
    },
    "types": ["@mui/material", "@emotion/react", "@emotion/styled"]
  },
  "include": ["src"]
}
